{% extends 'base.html.twig' %}

{% block body %}
    {% block header %}{{ parent() }}{% endblock %}
    <h1>Packagestatuses list</h1>

    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Id</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for packageStatus in packageStatuses %}
            <tr>
                <td><a href="{{ path('packagestatus_show', { 'id': packageStatus.id }) }}">{{ packageStatus.name }}</a></td>
                <td>{{ packageStatus.id }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('packagestatus_show', { 'id': packageStatus.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('packagestatus_edit', { 'id': packageStatus.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('packagestatus_new') }}">Create a new packageStatus</a>
        </li>
    </ul>
{% endblock %}
