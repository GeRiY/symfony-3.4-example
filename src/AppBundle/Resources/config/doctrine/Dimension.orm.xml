<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Dimension" table="dimension">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="package" type="integer" column="package" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="width" type="integer" column="width" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="height" type="integer" column="height" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="length" type="integer" column="length" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
