<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Package" table="package">
    <indexes>
      <index name="package_fk5" columns="dimension"/>
      <index name="package_fk0" columns="start_address"/>
      <index name="package_fk1" columns="target_address"/>
      <index name="package_fk2" columns="current_position"/>
      <index name="package_fk3" columns="package_status"/>
      <index name="package_fk4" columns="transfer_status"/>
      <index name="package_fk6" columns="tag"/>
      <index name="package_fk7" columns="courier"/>
      <index name="package_fk8" columns="handler"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dimension" type="integer" column="dimension" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="requestDate" type="datetime" column="request_date" nullable="false"/>
    <field name="deliveredDate" type="datetime" column="delivered_date" nullable="false"/>
    <field name="tag" type="integer" column="tag" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="startAddress" target-entity="Address" fetch="LAZY">
      <join-columns>
        <join-column name="start_address" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="targetAddress" target-entity="Address" fetch="LAZY">
      <join-columns>
        <join-column name="target_address" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="currentPosition" target-entity="Position" fetch="LAZY">
      <join-columns>
        <join-column name="current_position" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="packageStatus" target-entity="PackageStatus" fetch="LAZY">
      <join-columns>
        <join-column name="package_status" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="transferStatus" target-entity="TransferStatus" fetch="LAZY">
      <join-columns>
        <join-column name="transfer_status" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="courier" target-entity="Courier" fetch="LAZY">
      <join-columns>
        <join-column name="courier" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="handler" target-entity="FosUser" fetch="LAZY">
      <join-columns>
        <join-column name="handler" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
